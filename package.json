{
  "name": "est-o-auth",
  "version": "2.0.0",
  "private": true,
  "description": "Use Estonian ID-card, Mobile-ID, Smart-ID or E-mail as OAuth authentication provider",
  "author": "Argo Roots <argo@roots.ee>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "lint": "eslint . --fix --config .config/eslint.config.js",
    "dev": "nuxt dev",
    "dev:ssl": "nuxt dev --https --ssl-cert .certs/localhost.crt --ssl-key .certs/localhost.key",
    "build": "nuxt build",
    "start": "node .output/server/index.mjs",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "cleanup": "nuxt cleanup"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "3.848.0",
    "@aws-sdk/client-ses": "3.848.0",
    "@aws-sdk/client-sns": "3.848.0",
    "@nuxt/eslint": "1.7.1",
    "@nuxtjs/i18n": "10.0.3",
    "@nuxtjs/tailwindcss": "6.14.0",
    "@web-eid/web-eid-library": "2.0.2",
    "bcrypt": "6.0.0",
    "eslint": "9.32.0",
    "eslint-plugin-tailwindcss": "3.18.2",
    "jsonwebtoken": "9.0.2",
    "nuxt": "4.0.1",
    "stripe": "18.3.0"
  }
}
