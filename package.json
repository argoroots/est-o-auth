{
  "name": "est-o-auth",
  "version": "2.0.0",
  "private": true,
  "description": "Use Estonian ID-card, Mobile-ID, Smart-ID or E-mail as OAuth authentication provider",
  "author": "Argo Roots <argo@roots.ee>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.ts --fix --ignore-path .gitignore",
    "dev": "nuxt dev",
    "dev:ssl": "nuxt dev --https --ssl-cert .certs/localhost.crt --ssl-key .certs/localhost.key",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "postinstall": "nuxt prepare",
    "cleanup": "nuxt cleanup"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "3.503.1",
    "@aws-sdk/client-ses": "3.503.1",
    "@aws-sdk/client-sns": "3.503.1",
    "@nuxtjs/eslint-config": "12.0.0",
    "@nuxtjs/i18n": "8.0.0",
    "@web-eid/web-eid-library": "2.0.2",
    "autoprefixer": "10.4.17",
    "bcrypt": "5.1.1",
    "eslint": "8.56.0",
    "eslint-config-standard": "17.1.0",
    "jsonwebtoken": "9.0.2",
    "nuxt": "3.10.0",
    "postcss": "8.4.33",
    "tailwindcss": "3.4.1"
  }
}
